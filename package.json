{
	"name": "open-graph-fetcher-obsidian-plugin",
	"version": "0.0.0.1",
	"description": "A plugin for Obsidian that allows you to fetch Open Graph data from a URL using OpenGraph.io.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"Obsidian",
		"Plugin",
		"Open Graph",
		"Metadata"
	],
	"author": "The Lossless Group",
	"license": "The Unlicense",
	"devDependencies": {
		"@types/node": "^24.0.12",
		"@typescript-eslint/eslint-plugin": "8.36.0",
		"@typescript-eslint/parser": "8.36.0",
		"builtin-modules": "5.0.0",
		"esbuild": "0.25.6",
		"eslint": "^9.30.1",
		"tslib": "2.8.1",
		"typescript": "5.8.3"
	},
	"dependencies": {
		"@modelcontextprotocol/sdk": "^1.15.0",
		"dev": "^0.1.3",
		"fastify": "^5.4.0",
		"obsidian": "latest",
		"zod": "^4.0.0"
	}
}
